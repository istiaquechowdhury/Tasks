{
  "ConnectionStrings": {
    "DefaultConnection": "Server=your_server_name;Database=your_database_name;TrustServerCertificate=True;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Authentication": {
    "Facebook": {
      "ClientId": "your_facebook_client_id_here",
      "ClientSecret": "your_facebook_client_secret_here"
    },
    "Google": {
      "ClientId": "your_google_client_id_here",
      "ClientSecret": "your_google_client_secret_here"
    }
  },
  "AllowedHosts": "*"
}
